            fx:controller="form.LoginForm">
    <children>
        <Label layoutX="119.0" layoutY="151.0" text="%username"/>
        <Label layoutX="119.0" layoutY="193.0" text="%password"/>
        <TextField fx:id="_Username" layoutX="264.0" layoutY="147.0"/>
        <TextField fx:id="_Password" layoutX="264.0" layoutY="189.0"/>
        <Button fx:id="_Login" layoutX="136.0" layoutY="263.0" mnemonicParsing="false" text="%login"/>
        <Button fx:id="_Exit" layoutX="317.0" layoutY="263.0" mnemonicParsing="false" text="%exit"/>
    </children>
</AnchorPane>
